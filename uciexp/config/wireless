config wifi-device 'wl1'
        option disabled '0'
        option type 'broadcom'
        option beamforming '1'
        option hwmode 'a n ac'
        option channel 'auto'
        option htmode '80'
        option wme_apsd '0'
        option mu_mimo '0'
        option maxassoc '16'
        option frameburst '1'
        option wme_no_ack '1'
        option basic_rate '6.0 9.0 12.0 36.0'
        option transmit_rate '6.0 9.0 12.0 36.0'
        option ht_mcs 'e027'
        option vht_mcs_1 '13'
        option vht_mcs_2 '13'
        option vht_mcs_3 '13'
        option cts_protection_mode '2'
        option beacon_int '100'
        option dtim_period '1'
        option frag '2346'
        option rts '2347'
        option txpower '100'

config wifi-device 'wl0'
        option disabled '0'
        option type 'broadcom'
        option beamforming '1'
        option hwmode 'b g n'
        option channel 'auto'
        option htmode '20/40+'
        option wme_apsd '0'
        option maxassoc '16'
        option frameburst '1'
        option wme_no_ack '1'
        option basic_rate '1.0 2.0 5.5 11.0'
        option transmit_rate '1.0 2.0 5.5 11.0'
        option ht_mcs 'e027'
        option cts_protection_mode '2'
        option beacon_int '100'
        option dtim_period '1'
        option frag '2346'
        option rts '2347'
        option txpower '100'

config wifi-iface
        option device 'wl0'
        option mode 'ap'
        option ssid 'ciscosb'
        option disabled '1'
        option hidden '1'
        option encryption 'none'
        option mac_filter '0'
        option macpolicy '0'
        option vlanid '1'
        option isolate '0'
        option wmm '0'
        option wps '0'
        option captive '0'

config wifi-iface
        option device 'wl1'
        option mode 'ap'
        option ssid 'ciscosb1'
        option disabled '1'
        option hidden '1'
        option encryption 'none'
        option mac_filter '0'
        option macpolicy '0'
        option vlanid '1'
        option isolate '0'
        option wmm '0'
        option wps '0'
        option captive '0'

